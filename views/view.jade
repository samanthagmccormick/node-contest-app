extends layout

block content
	h1 Most Emotional Time-Lapse Vimeo Ever

	.container.center
		h3 All Videos
		a(href="/vote") Want to vote?

	.container
		for submission in submissions
			.col-lg-4
				iframe(src="#{submission.url}", frameborder="0", width="300", height="181", allowfullscreen) 
				h3: em Submitted by #{submission.name}

